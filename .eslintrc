{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "destructuring": true,
    "jsx": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "spread": true,
    "templateStrings": true
  },
  "env": {
    "amd": true,
    "browser": true,
    "mocha": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "$": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "block-scoped-var": 1,
    "brace-style": [
      1,
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": 0,
    "comma-style": [
      1,
      "last"
    ],
    "comma-spacing": 1,
    "curly": 1,
    "default-case": 1,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": 1,
    "guard-for-in": 1,
    "key-spacing": 1,
    "new-cap": 2,
    "new-parens": 2,
    "no-caller": 1,
    "no-comma-dangle": 2,
    "no-debugger": 1,
    "no-empty": 2,
    "no-extra-bind": 0,
    "no-floating-decimal": 2,
    "no-irregular-whitespace": 1,
    "no-lonely-if": 1,
    "no-loop-func": 0,
    "no-mixed-requires": 0,
    "no-mixed-spaces-and-tabs": [
      1,
      "smart-tabs"
    ],
    "no-multi-spaces": [
      1,
      {
        "exceptions": {
          "VariableDeclarator": true
        }
      }
    ],
    "no-multi-str": 1,
    "no-multiple-empty-lines": [
      1,
      {
        "max": 1
      }
    ],
    "no-new": 2,
    "no-redeclare": 1,
    "no-space-before-semi": 1,
    "no-extra-semi": 1,
    "no-script-url": 0,
    "no-shadow": 1,
    "no-trailing-spaces": 1,
    "no-undefined": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 1,
    "no-unused-vars": [
      0,
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-use-before-define": 2,
    "quote-props": [
      1,
      "as-needed"
    ],
    "quotes": [
      1,
      "single"
    ],
    "radix": 2,
    "react/jsx-quotes": [1, "double"],
    "react/jsx-no-undef": 1,
    "react/jsx-sort-props": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-did-mount-set-state": 0,
    "react/no-did-update-set-state": 0,
    "react/no-multi-comp": 1,
    "react/no-unknown-property": 2,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 1,
    "react/self-closing-comp": 1,
    "react/wrap-multilines": 2,
    "semi": 1,
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-before-function-parentheses": 1,
    "space-in-brackets": [
      0,
      "always",
      {
        "singleValue": false,
        "arraysInArrays": false,
        "propertyName": false
      }
    ],
    "space-infix-ops": 1,
    "space-unary-ops": 1,
    "space-return-throw-case": 1,
    "strict": 0,
    "valid-jsdoc": [
      2,
      {
        "prefer": {
          "return": "returns"
        }
      }
    ],
    "valid-typeof": 2,
    "wrap-iife": 2
  }
}
